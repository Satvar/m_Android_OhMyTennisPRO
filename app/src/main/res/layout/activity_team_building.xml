<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.appcompat.widget.Toolbar
        android:visibility="gone"
        android:id="@+id/toolbar_indi"
        android:background="@color/colorPrimary"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/back"
            android:src="@drawable/back_icon"
            android:tint="@color/whitecolor"
            android:layout_marginRight="10dp"
            android:layout_gravity="left"
            android:layout_width="25dp"
            android:layout_height="25dp" />
        <TextView
            android:textColor="@color/whitecolor"
            android:textStyle="bold"
            android:textSize="20dp"
            android:layout_gravity="center"
            android:text="Cours collectif"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </androidx.appcompat.widget.Toolbar>

    <ScrollView
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activity.collectivecourse.CollectiveCourseActivity">
        <LinearLayout
            android:orientation="vertical"
            android:layout_marginLeft="@dimen/login_logo_left_right"
            android:layout_marginRight="@dimen/login_logo_left_right"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_marginTop="10dp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_marginLeft="4dp"
                        android:textSize="16sp"
                        android:textColor="@color/blackcolor"
                        android:layout_marginBottom="0dp"
                        android:text="@string/description"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="@string/star"
                        android:textColor="@color/redcolor"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/teambuilding_description_error"
                    app:hintEnabled="false"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:errorTextAppearance="@style/ErrorText"
                    app:errorEnabled="false">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/teambuilding_description"
                        android:scrollbars="vertical"
                        android:inputType="textMultiLine"
                        android:textSize="15sp"
                        android:textColor="#000000"
                        android:hint="@string/description"
                        android:drawableTint="@color/colorPrimary"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:backgroundTint="@color/colorPrimary"/>
                </com.google.android.material.textfield.TextInputLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="10dp"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_marginLeft="4dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="0dp"
                    android:text="@string/type_image"
                    android:textColor="@color/blackcolor"
                    android:textSize="16sp" />
                <TextView
                    android:text="@string/star"
                    android:textColor="@color/redcolor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_weight="1"
                        android:id="@+id/teambuilding_image_error"
                        app:hintEnabled="false"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:errorTextAppearance="@style/ErrorText"
                        app:errorEnabled="false">
                        <com.google.android.material.textfield.TextInputEditText
                            android:inputType="number"
                            android:id="@+id/teambuilding_image"
                            android:scrollbars="vertical"
                            android:textSize="15sp"
                            android:textColor="#000000"
                            android:hint="Image de Evenement"
                            android:drawableTint="@color/colorPrimary"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:backgroundTint="@color/colorPrimary"/>
                    </com.google.android.material.textfield.TextInputLayout>
                    <Button
                        android:id="@+id/teambuilding_image_upload"
                        android:textColor="@color/white"
                        android:backgroundTint="@color/attach"
                        android:text="telecharger"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                </LinearLayout>
            </LinearLayout>

            <ImageView
                android:id="@+id/teambuilding_image_preview"
                android:visibility="gone"
                android:elevation="1dp"
                android:layout_margin="10dp"
                android:layout_gravity="center"
                android:layout_width="250dp"
                android:layout_height="200dp"/>
            <LinearLayout
                android:layout_marginTop="10dp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_marginLeft="4dp"                        android:textSize="16sp"
                        android:textColor="@color/blackcolor"
                        android:layout_marginBottom="0dp"
                        android:text="@string/transport_hint"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="@string/star"
                        android:textColor="@color/redcolor"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <CheckBox
                            android:id="@+id/teambuilding_car"
                            android:text="@string/transport_1"
                            android:buttonTint="@color/colorPrimary"
                            android:layout_weight="1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                        <CheckBox
                            android:id="@+id/teambuilding_bike"
                            android:text="@string/transport_3"
                            android:buttonTint="@color/colorPrimary"
                            android:layout_weight="1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <CheckBox
                            android:id="@+id/teambuilding_train"
                            android:text="@string/transport_2"
                            android:buttonTint="@color/colorPrimary"
                            android:layout_weight="1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                        <CheckBox
                            android:id="@+id/teambuilding_bus"
                            android:text="@string/transport_4"
                            android:buttonTint="@color/colorPrimary"
                            android:layout_weight="1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                </LinearLayout>

                <EditText
                    android:visibility="gone"
                    android:textSize="15sp"
                    android:id="@+id/cc_transport"
                    android:focusable="false"
                    android:drawableRight="@drawable/ic_spinner_arrow_down_black_24dp"
                    android:textColor="#000000"
                    android:hint="@string/transport_hint"
                    android:drawableTint="@color/colorPrimary"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:backgroundTint="@color/colorPrimary"
                    />
            </LinearLayout>



            <LinearLayout
                android:layout_marginTop="10dp"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_marginLeft="4dp"

                    android:textSize="16sp"
                    android:textColor="@color/blackcolor"
                    android:text="@string/select_the_plan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <TextView
                    android:text="@string/star"
                    android:textColor="@color/redcolor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <RadioGroup
                android:layout_marginTop="8dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RadioButton
                    android:checked="true"
                    android:id="@+id/teambuilding_commission"
                    android:text="@string/commission"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <RadioButton
                    android:visibility="gone"
                    android:id="@+id/teambuilding_abonnement"
                    android:text="@string/subscription"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
            </RadioGroup>
            <LinearLayout
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textColor="@color/redcolor"
                    android:text="@string/star"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:textSize="14sp"
                    android:gravity="center"
                    android:text="Champs obligatoires"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/save_button_hide_show"
                android:layout_marginTop="8dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <Button
                    android:textSize="15sp"
                    android:textAllCaps="true"
                    android:id="@+id/teambuilding_cancel"
                    android:layout_marginBottom="40dp"
                    android:layout_weight="1"
                    android:text="@string/CCANCEL"
                    android:textColor="@color/whitecolor"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginRight="5dp"
                    android:background="@drawable/cancelbutton"/>
                <Button
                    android:id="@+id/teambuilding_save"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginBottom="40dp"
                    android:layout_weight="1"
                    android:layout_marginLeft="5dp"
                    android:textColor="@color/whitecolor"
                    android:text="@string/save"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:background="@drawable/loginbutton"/>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
