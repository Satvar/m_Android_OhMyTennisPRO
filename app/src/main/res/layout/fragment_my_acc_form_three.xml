<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".fragment.myaccount.MyAccFormThreeFragment">
    <Button
        android:backgroundTint="@color/attach"
        android:id="@+id/modifier_three"
        android:layout_marginTop="20dp"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginRight="@dimen/login_logo_left_right"
        android:textSize="15dp"
        android:textAllCaps="true"

        android:text="Modifier"
        android:drawableTint="@color/whitecolor"
        android:textColor="@color/whitecolor"
        android:drawableRight="@drawable/edit_icon_16"
        android:paddingRight="15dp"
        android:layout_gravity="right"
        android:layout_marginBottom="15dp"
        android:background="@drawable/loginbutton"/>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_marginLeft="@dimen/login_logo_left_right"
                android:layout_marginRight="@dimen/login_logo_left_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/blackcolor"

                        android:layout_marginLeft="5dp"
                        android:layout_marginBottom="-6dp"
                        android:text="@string/payment"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="@string/star"
                        android:textColor="@color/redcolor"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <CheckBox
                                android:id="@+id/paymentcheck_1"
                                android:text="@string/payment_1"
                                android:buttonTint="@color/colorPrimary"
                                android:layout_weight="1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />
                            <CheckBox
                                android:id="@+id/paymentcheck_3"
                                android:text="@string/payment_3"
                                android:buttonTint="@color/colorPrimary"
                                android:layout_weight="1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <CheckBox
                                android:id="@+id/paymentcheck_2"
                                android:text="@string/payment_2"
                                android:buttonTint="@color/colorPrimary"
                                android:layout_weight="1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />
                            <CheckBox
                                android:visibility="gone"
                                android:text="@string/transport_4"
                                android:buttonTint="@color/colorPrimary"
                                android:layout_weight="1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />
                        </LinearLayout>
                    </LinearLayout>

                    <com.tech.cloudnausor.ohmytennispro.utils.MyAutoCompleteTextView
                       android:visibility="gone"
                        android:textColor="#000000"
                       android:id="@+id/form_three_payment"
                       android:focusable="false"
                       android:inputType="textEmailAddress"
                       android:drawableRight="@drawable/ic_spinner_arrow_down_black_24dp"
                       android:hint="@string/payment"
                       android:drawableTint="@color/colorPrimary"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:backgroundTint="@color/colorPrimary"
                        />
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:visibility="gone"
                    android:layout_height="wrap_content"

                    android:textColor="@color/redcolor"/>
            </LinearLayout>

            <TextView
                android:textColor="@color/colorPrimary"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="@dimen/login_logo_left_right"
                android:layout_marginRight="@dimen/login_logo_left_right"
                android:textSize="18dp"
                android:text="@string/bank_details"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <LinearLayout
                android:layout_marginTop="20dp"
                android:layout_marginLeft="@dimen/login_logo_left_right"
                android:layout_marginRight="@dimen/login_logo_left_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/blackcolor"
                        android:layout_marginLeft="5dp"
                        android:layout_marginBottom="-6dp"
                        android:text="@string/name_of_the_bank"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="@string/star"
                        android:textColor="@color/redcolor"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    </LinearLayout>
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/form_three_bank_name_error"
                        app:hintEnabled="false"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:errorTextAppearance="@style/ErrorText"
                        app:errorEnabled="false">
                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/form_three_bank_name"
                            android:textColor="#000000"
                            android:inputType="textCapWords"
                            android:hint="IBAN"
                            android:drawableTint="@color/colorPrimary"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:backgroundTint="@color/colorPrimary"/>
                    </com.google.android.material.textfield.TextInputLayout>

                    <!--<EditText-->
                        <!--android:id="@+id/form_three_bank_name"-->
                        <!--android:textColor="#000000"-->
                        <!--android:inputType="textEmailAddress"-->
                        <!--android:hint="@string/name_of_the_bank"-->
                        <!--android:drawableTint="@color/colorPrimary"-->
                        <!--android:layout_width="match_parent"-->
                        <!--android:layout_height="wrap_content"-->
                        <!--android:backgroundTint="@color/colorPrimary"-->
                         <!--/>-->
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:visibility="gone"
                    android:layout_height="wrap_content"
                    android:textColor="@color/redcolor"/>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="20dp"
                android:layout_marginLeft="@dimen/login_logo_left_right"
                android:layout_marginRight="@dimen/login_logo_left_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/blackcolor"
                        android:layout_marginLeft="5dp"
                        android:layout_marginBottom="-6dp"
                        android:text="@string/account_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="@string/star"
                        android:textColor="@color/redcolor"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    </LinearLayout>
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/form_three_bank_account_error"
                        app:hintEnabled="false"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:errorTextAppearance="@style/ErrorText"
                        app:errorEnabled="false">
                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/form_three_bank_account"
                            android:textColor="#000000"
                            android:inputType="textCapWords"
                            android:hint="@string/account_number"
                            android:drawableTint="@color/colorPrimary"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:backgroundTint="@color/colorPrimary"/>
                    </com.google.android.material.textfield.TextInputLayout>

                    <!--<EditText-->
                        <!--android:id="@+id/form_three_bank_account"-->
                        <!--android:textColor="#000000"-->
                        <!--android:inputType="number"-->
                        <!--android:hint="@string/account_number"-->
                        <!--android:drawableTint="@color/colorPrimary"-->
                        <!--android:layout_width="match_parent"-->
                        <!--android:layout_height="wrap_content"-->
                        <!--android:backgroundTint="@color/colorPrimary"-->
                         <!--/>-->
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:visibility="gone"
                    android:layout_height="wrap_content"

                    android:textColor="@color/redcolor"/>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="20dp"
                android:layout_marginLeft="@dimen/login_logo_left_right"
                android:layout_marginRight="@dimen/login_logo_left_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                    <TextView

                        android:textColor="#000000"
                        android:layout_marginLeft="5dp"
                        android:layout_marginBottom="-6dp"
                        android:text="@string/bank_code"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="@string/star"
                        android:textColor="@color/redcolor"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    </LinearLayout>
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/form_three_bank_code_error"
                        app:hintEnabled="false"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:errorTextAppearance="@style/ErrorText"
                        app:errorEnabled="false">
                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/form_three_bank_code"
                            android:textColor="#000000"
                            android:inputType="textCapWords"
                            android:hint="@string/bank_code"
                            android:drawableTint="@color/colorPrimary"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:backgroundTint="@color/colorPrimary"/>
                    </com.google.android.material.textfield.TextInputLayout>

                    <!--<EditText-->
                        <!--android:id="@+id/form_three_bank_code"-->
                        <!--android:textColor="#000000"-->
                        <!--android:inputType="textEmailAddress"-->
                        <!--android:hint="@string/bank_code"-->
                        <!--android:drawableTint="@color/colorPrimary"-->
                        <!--android:layout_width="match_parent"-->
                        <!--android:layout_height="wrap_content"-->
                        <!--android:backgroundTint="@color/colorPrimary"-->
                         <!--/>-->
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:visibility="gone"
                    android:layout_height="wrap_content"

                    android:textColor="@color/redcolor"/>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="20dp"
                android:layout_marginLeft="@dimen/login_logo_left_right"
                android:layout_marginRight="@dimen/login_logo_left_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                    <TextView
                        android:textColor="#000000"

                        android:layout_marginLeft="5dp"
                        android:layout_marginBottom="-6dp"
                        android:text="@string/city_of_the_bank"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="@string/star"
                        android:textColor="@color/redcolor"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    </LinearLayout>
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/form_three_bank_city_error"
                        app:hintEnabled="false"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:errorTextAppearance="@style/ErrorText"
                        app:errorEnabled="false">
                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/form_three_bank_city"
                            android:textColor="#000000"
                            android:inputType="textCapWords"
                            android:hint="@string/city_of_the_bank"
                            android:drawableTint="@color/colorPrimary"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:backgroundTint="@color/colorPrimary"/>
                    </com.google.android.material.textfield.TextInputLayout>

                    <!--<EditText-->
                        <!--android:id="@+id/form_three_bank_city"-->
                        <!--android:textColor="#000000"-->
                        <!--android:inputType="textEmailAddress"-->
                        <!--android:hint="@string/city_of_the_bank"-->
                        <!--android:drawableTint="@color/colorPrimary"-->
                        <!--android:layout_width="match_parent"-->
                        <!--android:layout_height="wrap_content"-->
                        <!--android:backgroundTint="@color/colorPrimary"-->
                         <!--/>-->
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:visibility="gone"
                    android:layout_height="wrap_content"

                    android:textColor="@color/redcolor"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textColor="@color/redcolor"
                    android:text="@string/star"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="Champs obligatoires"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="@dimen/login_logo_left_right"
                android:layout_marginRight="@dimen/login_logo_left_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <Button
                    android:id="@+id/form_three_save"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:textAllCaps="true"
                    android:textSize="16sp"
                    android:layout_height="40dp"
                    android:layout_marginTop="30sp"
                    android:layout_marginBottom="30sp"
                    android:textStyle="bold"
                    android:textColor="@color/whitecolor"
                    android:text="@string/submit"
                    android:background="@drawable/loginbutton"
                    />

            </LinearLayout>


        </LinearLayout>
    </ScrollView>

</LinearLayout>




